<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="container" prefHeight="70.0" style="-fx-border-image-width: 1; -fx-border-color: black;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <left>
        <VBox prefHeight="200.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="fullName" text="Label" wrapText="true">
                    <font>
                        <Font name="Calibri" size="15.0" />
                    </font>
                </Label>
                <Label fx:id="age" wrapText="true">
                    <font>
                        <Font name="Calibri" size="11.0" />
                    </font>
                </Label>
                <Label fx:id="profession" text="Безработный" wrapText="true">
                    <font>
                        <Font name="Calibri" size="11.0" />
                    </font>
                </Label>
                <Label fx:id="organization" text="-" wrapText="true">
                    <font>
                        <Font name="Calibri" size="11.0" />
                    </font>
                </Label>
                <Label fx:id="salary" wrapText="true">
                    <font>
                        <Font name="Calibri" size="11.0" />
                    </font>
                </Label>
            </children>
        </VBox>
    </left>
    <right>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="coefficient" text="Label" wrapText="true">
                    <font>
                        <Font name="Calibri" size="14.0" />
                    </font>
                </Label>
            </children>
        </VBox>
    </right>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</BorderPane>
